#!/bin/bash

whitelist() {
  cat <<END

awscli
bat
coreutils
dos2unix
dust
ffmpeg
findutils
fswatch
fzf
gawk
gcal
git
gnu-sed
gnu-tar
gnupg
go
graphviz
htop
imagemagick
iproute2mac
jpalardy/tap/memora
jq
mtr
nmap
node
pandoc
parallel
pgformatter
pstree
pup
pwgen
qrencode
ripgrep
rlwrap
shellcheck
tidy-html5
tmux
utimer
vim
wget
yamllint

END
}

brew leaves | grep -v -f <(whitelist | grep .)

